  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Build bee",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bee"
      , "args": [ "build" ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Install bee",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bee"
      , "args": [ "-v", "--nofail", "install" ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug bee test",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bee"
      , "args": [ "test" ]
    }
  ]
}